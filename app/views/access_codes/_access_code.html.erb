<% if local_assigns[:show] == "tr" %>
  <%= render partial: "access_codes/displays/tr", locals: { access_code: access_code } %>
<% elsif local_assigns[:show] == "li" %>
    <%= render partial: "access_codes/displays/li", locals: { access_code: access_code } %>
<% elsif local_assigns[:show] == "div" %>
  <%= render partial: "access_codes/displays/div", locals: { access_code: access_code } %>
<% else %>
  <%= render partial: "access_codes/displays/public", locals: { access_code: access_code } %>
<% end %>
