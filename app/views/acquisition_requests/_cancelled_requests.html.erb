<h2>Recently Cancelled Requests (200)</h2>
<div class="list-group">
  <% @recently_cancelled.each do |r| %>
    <a class="list-group-item" href="<%= acquisition_request_path(r) %>">
      <span class="badge small">Cancelled By <%= r.cancelled_by.name rescue nil %></span>
      <h5 class="list-group-item-heading"><%= r.item.title rescue nil %></h5>
      <p class="list-group-item-text weak">Cancelled on <%= r.cancelled_at.strftime("%b %e, %Y") rescue nil%></p>
    </a>
  <% end %>
</div>
