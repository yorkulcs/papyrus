$("#scan_list_<%= @scan_list.id %>_item_form_holder").empty();
<% if @scan_list.errors.size == 0 %>
  $("#scan_list_<%= @scan_list.id %> .scan_list_items").append('<%=escape_javascript render @scan_item, show: "div" %>')
  $(".blank-slate").hide();
<% else %>
  $('#scan_list_<%= @scan_list.id %>_item_form_holder').prepend('<%=escape_javascript render("scan_items/form") %>');
<% end %>
