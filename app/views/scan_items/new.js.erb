$("#scan_list_<%= @scan_list.id %>_item_form_holder").empty();
<% if @scan_item.item_id == nil %>
  $('#scan_list_<%= @scan_list.id %>_item_form_holder').prepend('<%=escape_javascript render("scan_items/form") %>');
  setup_item_token_input("#scan_item_item_id", "<%= search_items_path %>", 1);
<% else %>
  $('#scan_list_<%= @scan_list.id %>_item_form_holder').prepend('<%=escape_javascript render("scan_items/form_with_item") %>');
<% end %>

$(".datepicker" ).datepicker({ dateFormat: 'yy-mm-dd' });
