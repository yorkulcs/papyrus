<% if local_assigns[:show] == "tr" %>
  <%= render partial: "scan_lists/displays/tr", locals: { scan_list: scan_list } %>
<% elsif local_assigns[:show] == "li" %>
    <%= render partial: "scan_lists/displays/li", locals: { scan_list: scan_list } %>
<% elsif local_assigns[:show] == "div" %>
  <%= render partial: "scan_lists/displays/div", locals: { scan_list: scan_list } %>
<% else %>
  <%= render partial: "scan_lists/displays/public", locals: { scan_list: scan_list } %>
<% end %>
