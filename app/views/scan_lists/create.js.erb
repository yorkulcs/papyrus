$("#scan_list_form_holder").empty();
<% if @scan_list.errors.size == 0 %>
  $("#scan_lists").prepend('<%=escape_javascript render @scan_list, show: "div" %>')
  $(".blank-slate").hide();
<% else %>
  $('#scan_list_form_holder').prepend('<%=escape_javascript render("scan_lists/form") %>');
<% end %>
