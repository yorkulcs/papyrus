<% if local_assigns[:show] == "tr" %>
  <%= render partial: "students/displays/tr", locals: { student: student } %>
<% elsif local_assigns[:show] == "li" %>
    <%= render partial: "students/displays/li", locals: { student: student } %>
<% elsif local_assigns[:show] == "div" %>
  <%= render partial: "students/displays/div", locals: { student: student } %>
<% else %>
  <%= render partial: "students/displays/public", locals: { student: student } %>
<% end %>
