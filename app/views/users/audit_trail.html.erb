
<% title "Audit Trail / #{@user.name}", false %>
<div class="row">
	<div class="col-md-12">
		<%= panel(title: "Audit Trail / #{@user.name} ", icon: "history") do %>
			<div class="student-header row">
				<div class="col-md-10">
					<h4>
						<a class="btn btn-xs btn-default" href="<%= users_path() %>" title="Back To Student Details">
							<i class="fa fa-chevron-left"></i>
						</a>
						<%= @user.name %>
					</h4>
				</div>
			</div>
			<hr/>
			<%= render partial:"audit_trail/trail", locals: {audits_grouped: @audits_grouped} %>
		<% end %>
	</div>
</div>
